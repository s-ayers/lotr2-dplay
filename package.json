{
  "name": "dplay",
  "version": "1.0.0",
  "description": "Direct Play middleware.",
  "scripts": {
    "test": " ts-mocha  src/**/*.test.ts --timeout 10000 --exit",
    "test:coverage": "nyc npm run test"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "**/*.test.ts"
    ],
    "reporter": [
      "html",
      "lcovonly",
      "text",
      "text-summary"
    ],
    "all": true
  },
  "keywords": [],
  "author": "Steve Ayers",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "binary-parser": "^2.1.0",
    "bufferutil": "^4.0.1",
    "chalk": "^4.1.2",
    "ether-frame": "^0.2.0",
    "pcap-ng-parser": "^1.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "mock-udp": "^0.1.1",
    "nyc": "^14.1.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^8.2.0",
    "typescript": "^4.6.4",
    "typings": "^2.1.1"
  }
